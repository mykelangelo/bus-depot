<?xml version="1.0" encoding="UTF-8"?>
<!--@formatter:off-->
<configuration>
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>
                %d{yyyy-MM-dd HH:mm:ss.SSS} %blue([%X]) %highlight(%5p -- [%15.15t]) %cyan(%40.40logger{39}) : %highlight(%m%n%xEx)%n
            </pattern>
        </encoder>
    </appender>
    <root level="WARN">
        <appender-ref ref="CONSOLE"/>
    </root>

    <logger name="com.papenko.project" level="TRACE"/>
</configuration>